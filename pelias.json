{
  "esclient": {
    "apiVersion": "6.8",
    "keepAlive": true,
    "requestTimeout": "20000",
    "hosts": [
      {
        "env": "dev",
        "protocol": "http",
        "host": "localhost",
        "port": 9200
      }
    ],
    "log": [{
      "type": "stdio",
      "level": [ "error", "warning" ]
    }]
  },
  "elasticsearch": {
    "settings": {
      "index": {
        "number_of_replicas": "0",
        "number_of_shards": "1",
        "refresh_interval": "1m"
      }
    }
   },
  "logger": {
    "level": "info",
    "suspectFile": false
  },
  "acceptance-tests": {
    "endpoints": { "local": "http://localhost:3100/v1/" }
  },

  "imports": {
    "defaultAdminLayers": ["localadmin", "locality", "neighbourhood", "region", "postalcode"],
    "defaultCountry": {
      "name": "Romania",
      "abbr": "ROM"
    },
    "adminLookup": {
      "enabled": true,
      "maxConcurrentReqs": 4,
      "localizedNames": true,
      "dropUnmapped": true
    },
    "openstreetmap": {
      "datapath": "/mnt/data/openstreetmap/",
      "leveldbpath": "/tmp",
      "import": [
        {
          "filename": "romania-latest.osm.pbf",
          "importVenues": true
        }
      ]
    },
    "openaddresses": {
      "datapath": "/mnt/data/openaddresses/",
      "adminLayers": ["localadmin", "locality", "neighbourhood", "region"]
    },

    "whosonfirst": {
      "datapath": "/mnt/data/wof_data"
    },
    "wofPipNames": {
      "localadmin": ["name:rom_x_preferred", "name:rom_x_preferred"],
      "region": ["name:rom_x_preferred", "name:rom_x_preferred"],
      "neighbourhood": ["name:rom_x_preferred", "name:rom_x_preferred"]
    }
  },
  "schema": {
    "indexName": "pelias"
  },
  "api" : {
    "languages": [
      "ro",
      "default",
      "en",
      "local",
      "alternative",
      "international",
      "national",
      "official",
      "old",
      "regional",
      "short",
      "sorting"
    ]
  }
}
